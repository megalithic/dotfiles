#!/usr/bin/env bash

TMUX_VAR="${1:-}"

tmux_get_var() {
    local key=$1
    tmux showenv | awk -F= -v key="$key" '$1==key {print $2}'
}

var_value=$(tmux_get_var "$TMUX_VAR")

if [ "$var_value" != "" ]; then
    echo "$var_value"
else
    if [ "$TMUX_VAR" == "SESSION_ICON" ]; then
        echo "Ôíê"
    elif [ "$TMUX_VAR" == "SESSION_FG" ]; then
        echo "#d9bb80"
    elif [ "$TMUX_VAR" == "SESSION_BG" ]; then
        echo "#3c474d"
    fi
fi
