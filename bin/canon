#!/usr/bin/env zsh

session="canonize"

tmux new -s $session -d
tmux switch -t $session
tmux send-keys -t $session "cd ~;" C-m
# tmux split-window -h -t $session
tmux send-keys -t $session "cd ~; teamocil --here $session" C-m

