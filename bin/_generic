#!/bin/zsh

# find generic installers and run them interactively
echo ""
echo ":: ------ running generic installers..."
echo ""

# some generic installers need a zshrc to exist.. ¯\_(ツ)_/¯
touch ~/.zshrc

find $DOTS -iname all.sh | while read installer ; do echo "-- running installer: $installer"; sh -c "${installer}" ; done
