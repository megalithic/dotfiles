{
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "workbench.colorTheme": "Everforest Dark",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "debug.console.fontFamily": "'JetBrainsMono Nerd Font Mono', monospace",
  "diffEditor.renderSideBySide": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.detectIndentation": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 1.6,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": true,
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    ".env": ".env, .env.example",
    ".gitignore": ".gitignore, .dockerignore",
    "vite.config.ts": "vitest.config.js, vitest.config.ts, vite.config.js, vite.config.ts",
    "tsconfig.json": "tsconfig.json, jsconfig.json",
    "*.csproj": "*.csproj, nuget.config",
    "*.razor": "$(capture).razor.css, $(capture).razor.cs",
    "*.cshtml": "$(capture).cshtml.cs"
  },
  "editor.renderWhitespace": "boundary",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "git.allowForcePush": true,
  "git.branchProtection": ["master", "main"],
  "git.enableCommitSigning": true,
  "git.showPushSuccessNotification": true,
  "git.suggestSmartCommit": false,
  "javascript.referencesCodeLens.enabled": true,
  "notebook.lineNumbers": "on",
  "notebook.output.textLineLimit": 0,
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontWeight": 400,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.tabs.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 14,
  "workbench.tree.renderIndentGuides": "always",
  "search.exclude": {
    "**/.git": true,
    "**/.nuxt": true,
    "**/.svelte-kit": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/tmp": true,
    "**/yarn.lock": true,
    "**/bin": true,
    "**/obj": true
  },
  "files.associations": {
    "*.svx": "markdown",
    ".env*": "makefile",
    "*.svg": "html"
  },
  "editor.semanticHighlighting.enabled": true,
  "typescript.tsserver.log": "off",
  "workbench.editorAssociations": {
    "*.pdf": "latex-workshop-pdf-hook"
  },
  "explorer.confirmDelete": false
}
